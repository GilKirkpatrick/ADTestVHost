    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${file}"
        },
        {
            "name": "PowerShell: Interactive Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": ""
        },
        {
            "name": "Launch New-ADTestDomain",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/Launch-Script.ps1",
            "cwd": ""
        },
        {
            "name": "PowerShell: Debug Pester tests",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "args": [
                "JobProgressManager.Tests.ps1"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}